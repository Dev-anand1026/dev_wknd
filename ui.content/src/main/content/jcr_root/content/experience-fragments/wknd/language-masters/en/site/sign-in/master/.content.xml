<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/wknd/settings/wcm/templates/experience-fragment-web-variation-template"
        cq:xfMasterVariation="{Boolean}true"
        jcr:isCheckedOut="{Boolean}true"
        jcr:mixinTypes="[mix:versionable]"
        jcr:primaryType="cq:PageContent"
        jcr:title="Sign In (Modal)"
        jcr:uuid="f59734c1-f6b3-4bc6-9d6e-db061746fea4"
        sling:resourceType="wknd/components/structure/xfpage">
        <root
            cq:styleIds="[1569614530122]"
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <responsivegrid
                cq:styleIds="[1569950941796]"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
                <title
                    cq:styleIds="[1568996484405]"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Sign In"
                    sling:resourceType="wknd/components/content/title"/>
                <title_1943245182
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Welcome Back"
                    sling:resourceType="wknd/components/content/title"
                    type="h3"/>
                <sign_in_form
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wknd/components/content/sign-in-form"
                    actionUrl="/system/sling/login/j_security_check"
                    redirectUrl="/content/wknd/us/en.html">
                    <responsivegrid
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid">
                        <field-1
                            jcr:primaryType="nt:unstructured"
                            jcr:title="EMAIL"
                            sling:resourceType="wknd/components/form/text"
                            helpMessage="EMAIL"
                            hideTitle="true"
                            name="j_username"
                            rows="1"
                            type="text"
                            usePlaceholder="true"/>
                        <field-2
                            jcr:primaryType="nt:unstructured"
                            jcr:title="PASSWORD"
                            sling:resourceType="wknd/components/form/text"
                            helpMessage="PASSWORD"
                            hideTitle="true"
                            name="j_password"
                            rows="1"
                            type="password"
                            usePlaceholder="true"/>
                        <text-1
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wknd/components/content/text"
                            text="&lt;p>&lt;a href=&quot;#&quot;>FORGOT YOUR PASSWORD?&lt;/a>&lt;/p>&#xa;"
                            textIsRich="true"/>
                        <button-1
                            jcr:primaryType="nt:unstructured"
                            jcr:title="SIGN IN"
                            sling:resourceType="wknd/components/form/button"
                            type="submit"/>
                    </responsivegrid>
                </sign_in_form>
                <separator
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wknd/components/content/separator"/>
                <text
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wknd/components/content/text"
                    text="&lt;p>NEED AN ACCOUNT? &lt;a href=&quot;#sign-up&quot;>SIGN UP&lt;/a>&lt;/p>&#xa;"
                    textIsRich="true"/>
            </responsivegrid>
        </root>
        <cq:metadata jcr:primaryType="nt:unstructured"/>
    </jcr:content>
</jcr:root>
